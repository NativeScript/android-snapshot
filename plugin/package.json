{
  "name": "nativescript-dev-android-snapshot",
  "version": "0.0.5",
  "license": "Apache-2.0",
  "dependencies": {
    "adm-zip": "^0.4.7",
    "nativescript-hook": "^0.2.1",
    "semver": "^5.1.0",
    "shelljs": "^0.6.0"
  },
  "scripts": {
    "preuninstall": "node preuninstall.js",
    "postinstall": "node postinstall.js"
  },
  "nativescript": {
    "platforms": {
      "android": "2.1.0"
    },
    "hooks": [
      {
        "type": "before-prepare",
        "script": "hooks/before-prepare-hook.js",
        "inject": true
      },
      {
        "type": "after-prepare",
        "script": "hooks/after-prepare-hook.js",
        "inject": true
      }
    ]
  }
}
